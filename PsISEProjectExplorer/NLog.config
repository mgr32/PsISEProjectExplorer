<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <targets>
    <target name="logfile" xsi:type="File" fileName="${nlogdir}\PsISEProjectExplorer.log.txt" layout="${longdate}|${threadid}|${threadname}|${level:uppercase=true}|${callsite}|${message} ${exception:format=ToString}" />
  </targets>

  <rules>
    <logger name="*" minlevel="Error" writeTo="logfile" />
  </rules>
</nlog>